<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Log In</title>
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css">
    <link rel="stylesheet" href="/css/main.css" >
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/js-cookie/2.1.1/js.cookie.min.js" charset="utf-8"></script>
    <script src="/js/indexscripts.js" charset="utf-8"></script>
    <script src="/js/index.js" charset="utf-8"></script>
    <title>Address Book</title>
  </head>
  <body>
    <header class="header">
      <div>
        <h1>AN OLD FASHIONED ADDRESS BOOK...</h1>
      </div>
    <header>
    <div class="case" id="log-in">
      <h3>LOG IN...</h3>
      <div class="log-in">
          <div class="log-in-form" id="login-form">
            <div class="field">
              <input type="text" name="username" placeholder="Username">
            </div>
            <div class="field">
              <input type="password" name="password" placeholder="Password">
            </div>
            <div class="field">
              <input id="login" class="ui huge inverted button" type="submit" name="name" value="LOG IN">
            </div>
          </div >
          <button id="button">New, Sign Up!</button>
          </div>
        </div>
      </div>
    <div class="case" id="sign-up">

      <div class="sign-up-form">
        <div id="sign-on-up">
          <div class="field" id=first-field>
            <input type="text" name="username" placeholder="Username">
          </div>
          <div class="center aligned field">
            <input type="text" name="email" placeholder="Email">
          </div>
            <div class="field">
            <input type="password" name="password" placeholder="Password">
          </div>
          <div class="field">
            <input id="add-user" class="ui medium black button" type="submit" name="name" value="Submit">
          </div>
        </div>
      </div>
</div>
   <!-- Hidden sign up form -->







  </body>
</html>
